@model IEnumerable<Fitness.Models.User>

@if (Model.Count() > 0)
{
<table class="table">

    <tr>
        <th>
            @Html.Label("Name")
        </th>
        <th>
            @Html.Label("Options")
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FirstName @item.LastName
            </td>

            <td>
                @Html.ActionLink("Remove Friend", "Remove", new { id = item.UserID }, htmlAttributes: new { @class = "btn btn-primary" })
            </td>
        </tr>
    }

</table>
}